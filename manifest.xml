<!-- <?xml version="1.0" encoding="UTF-8" standalone="yes"?> -->
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
  xmlns:mailappor="http://schemas.microsoft.com/office/mailappversionoverrides/1.0" xsi:type="MailApp">
  <Id>28925bda-c992-48ce-93ef-eef615493c4b</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Contoso</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="outlook plugin"/>
  <Description DefaultValue="add-in."/>
  <IconUrl DefaultValue=" https://shahzadumar-w.github.io/Outlook-addin-plugin/assets/icon-64.png"/>
  <HighResolutionIconUrl DefaultValue=" https://shahzadumar-w.github.io/Outlook-addin-plugin/assets/icon-128.png"/>
  <SupportUrl DefaultValue="https://www.contoso.com/help"/>
  <AppDomains>
    <AppDomain>https://YourAppDomain</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>
  <Requirements>
    <Sets>
      <Set Name="Mailbox" MinVersion="1.1"/>
    </Sets>
  </Requirements>
  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue=" https://shahzadumar-w.github.io/Outlook-addin-plugin/taskpane.html"/>
        <RequestedHeight>250</RequestedHeight>
      </DesktopSettings>
    </Form>
  </FormSettings>
  <Permissions>ReadWriteItem</Permissions>
  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" ItemType="Message" FormType="Read"/>
  </Rule>
  <DisableEntityHighlighting>false</DisableEntityHighlighting>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides" xsi:type="VersionOverridesV1_0">
    <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides/1.1" xsi:type="VersionOverridesV1_1">
      <Requirements>
        <bt:Sets DefaultMinVersion="1.3">
          <bt:Set Name="Mailbox"/>
        </bt:Sets>
      </Requirements>
      <Hosts>
        <Host xsi:type="MailHost">
          <DesktopFormFactor>
            <FunctionFile resid="Commands.Url"/>
            <ExtensionPoint xsi:type="MessageReadCommandSurface">
              <OfficeTab id="TabDefault">
                <Group id="msgReadGroup">
                  <Label resid="GroupLabel"/>
                  
                  <!-- Button 1: Open Taskpane -->
                  <Control xsi:type="Button" id="msgReadOpenPaneButton">
                    <Label resid="TaskpaneButton.Label"/>
                    <Supertip>
                      <Title resid="TaskpaneButton.Label"/>
                      <Description resid="TaskpaneButton.Tooltip"/>
                    </Supertip>
                    <Icon>
                      <bt:Image size="16" resid="Icon.16x16"/>
                      <bt:Image size="32" resid="Icon.32x32"/>
                      <bt:Image size="80" resid="Icon.80x80"/>
                    </Icon>
                    <Action xsi:type="ShowTaskpane">
                      <SourceLocation resid="Taskpane.Url"/>
                      <SupportsPinning>true</SupportsPinning>
                    </Action>
                  </Control>

                  <!-- Button 2: Placeholder -->
                  <Control xsi:type="Button" id="msgReadPlaceholderButton1">
                    <Label resid="PlaceholderButton1.Label"/>
                    <Supertip>
                      <Title resid="PlaceholderButton1.Label"/>
                      <Description resid="PlaceholderButton1.Tooltip"/>
                    </Supertip>
                    <Icon>
                      <bt:Image size="16" resid="Icon.16x16"/>
                      <bt:Image size="32" resid="Icon.32x32"/>
                      <bt:Image size="80" resid="Icon.80x80"/>
                    </Icon>
                    <Action xsi:type="ExecuteFunction">
                      <FunctionName>placeholderFunction1</FunctionName>
                    </Action>
                  </Control>

                  <!-- Button 3: Placeholder -->
                  <Control xsi:type="Button" id="msgReadPlaceholderButton2">
                    <Label resid="PlaceholderButton2.Label"/>
                    <Supertip>
                      <Title resid="PlaceholderButton2.Label"/>
                      <Description resid="PlaceholderButton2.Tooltip"/>
                    </Supertip>
                    <Icon>
                      <bt:Image size="16" resid="Icon.16x16"/>
                      <bt:Image size="32" resid="Icon.32x32"/>
                      <bt:Image size="80" resid="Icon.80x80"/>
                    </Icon>
                    <Action xsi:type="ExecuteFunction">
                      <FunctionName>placeholderFunction2</FunctionName>
                    </Action>
                  </Control>

                </Group>
              </OfficeTab>
            </ExtensionPoint>
          </DesktopFormFactor>
        </Host>
      </Hosts>
      <Resources>
        <bt:Images>
          <bt:Image id="Icon.16x16" DefaultValue=" https://shahzadumar-w.github.io/Outlook-addin-plugin/assets/icon-16.png"/>
          <bt:Image id="Icon.32x32" DefaultValue=" https://shahzadumar-w.github.io/Outlook-addin-plugin/assets/icon-32.png"/>
          <bt:Image id="Icon.80x80" DefaultValue=" https://shahzadumar-w.github.io/Outlook-addin-plugin/assets/icon-80.png"/>
        </bt:Images>
        <bt:Urls>
          <bt:Url id="Commands.Url" DefaultValue=" https://shahzadumar-w.github.io/Outlook-addin-plugin/commands.html"/>
          <bt:Url id="Taskpane.Url" DefaultValue=" https://shahzadumar-w.github.io/Outlook-addin-plugin/taskpane.html"/>
        </bt:Urls>
        <bt:ShortStrings>
          <bt:String id="GroupLabel" DefaultValue="Add-in"/>
          <bt:String id="TaskpaneButton.Label" DefaultValue="Open Taskpane"/>
          <bt:String id="PlaceholderButton1.Label" DefaultValue="Placeholder 1"/>
          <bt:String id="PlaceholderButton2.Label" DefaultValue="Placeholder 2"/>
        </bt:ShortStrings>
        <bt:LongStrings>
          <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Opens the taskpane."/>
          <bt:String id="PlaceholderButton1.Tooltip" DefaultValue="Future action 1."/>
          <bt:String id="PlaceholderButton2.Tooltip" DefaultValue="Future action 2."/>
        </bt:LongStrings>
      </Resources>
    </VersionOverrides>
  </VersionOverrides>
</OfficeApp>
